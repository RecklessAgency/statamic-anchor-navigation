{
    "name": "visuellverstehen/statamic-anchor-navigation",
    "autoload": {
        "psr-4": {
            "VV\\AnchorNavigation\\": "src"
        }
    },
    "authors": [
        {
            "name": "visuellvertsehen"
        }
    ],
    "extra": {
        "statamic": {
            "name": "Statamic Anchor Headlines",
            "description": "Statamic Anchor Navigation addon generates a navigation from all level 2 headings from bard fieldtype."
        },
        "laravel": {
            "providers": [
                "Visuellverstehen\\StatamicAnchorNavigation\\ServiceProvider"
            ]
        }
    },
    "require": {
        "statamic/cms": "^3.4"
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    }
}
